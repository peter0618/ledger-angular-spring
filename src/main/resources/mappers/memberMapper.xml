<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.ledgerangularspring.mapper.MemberMapper">

    <select id="read" resultType="com.example.ledgerangularspring.domain.MemberVO">
        select *
          from tbl_member
         where userid = #{userid}
    </select>
</mapper>